{
    "name": "phpro/soap-client-sf2-bridge",
    "license": "MIT",
    "description": "A Symfony bridge for the phpro/soap-client package",
    "keywords": ["soap", "symfony", "datacollector", "debug"],
    "autoload": {
        "psr-0": {
            "Phpro\\SoapClient\\BridgeBundle": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/dependency-injection": "~3.0|~4.0",
        "symfony/event-dispatcher": "~3.0|~4.0",
        "symfony/http-kernel": "~3.0|~4.0",
        "symfony/stopwatch": "~3.0|~4.0",
        "phpro/soap-client": "~1.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpspec/phpspec": "~6",
        "squizlabs/php_codesniffer": "~3",
        "phpunit/phpunit": "9.*",
        "phpro/grumphp": "~0.19"
    }
}
