{
    "name": "phpro/soap-client-sf2-bridge",
    "license": "MIT",
    "description": "A Symfony2 bridge for the phpro/soap-client package",
    "keywords": ["soap", "symfony", "datacollector", "debug"],
    "autoload": {
        "psr-0": {
            "Phpro\\SoapClient\\BridgeBundle": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "symfony/dependency-injection": "^2.3",
        "symfony/event-dispatcher": "^2.3",
        "symfony/http-kernel": "^2.3",
        "symfony/stopwatch": "^2.3",
        "phpro/soap-client": "^0.2"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.2",
        "squizlabs/php_codesniffer": "~2.3",
        "phpunit/phpunit": "4.*",
        "phpro/grumphp": "~0.5"
    }
}
