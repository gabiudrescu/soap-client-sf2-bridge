{% extends '@WebProfiler/Profiler/layout.html.twig' %}


{% block toolbar %}
    {% if collector.calls | length > 0 %}
        {% set profiler_markup_version = profiler_markup_version|default(1) %}

        {% set icon %}
            {% if profiler_markup_version == 1 %}
                <img width="18" height="28" alt="Soap"
                     src="data:image/png;base64,"/>
                <span class="sf-toolbar-value sf-toolbar-status">{{ collector.calls | length }}</span>
                <span class="sf-toolbar-info-piece-additional-detail">in {{ '%0.2f'|format(collector.totalTiming) }} ms</span>
            {% else %}
                {{ include('@PhproSoapClientBridge/Icon/soap.svg') }}
                <span class="sf-toolbar-value">{{ collector.calls | length }}</span>
                <span class="sf-toolbar-info-piece-additional-detail">
                    <span class="sf-toolbar-label">in</span>
                    <span class="sf-toolbar-value">{{ '%0.2f'|format(collector.totalTiming) }}</span>
                    <span class="sf-toolbar-label">ms</span>
                    </span>
            {% endif %}
        {% endset %}

        {% set text %}
            <div class="sf-toolbar-info-piece">
                <b>Soap Requests</b>
                <span class="sf-toolbar-status">{{ collector.calls | length }}</span>
            </div>

            <div class="sf-toolbar-info-piece">
                <b>Total time</b>
                <span>{{ '%0.2f'|format(collector.totalTiming) }} ms</span>
            </div>

            <div class="sf-toolbar-info-piece">
                <ul>
                    {% for call in collector.calls %}
                        <li>{{ call.method }}: <strong>{{ call.timing }} ms</strong></li>
                    {% endfor %}
                </ul>
            </div>
        {% endset %}

        {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { link: true }) }}
    {% endif %}
{% endblock %}

{% block menu %}
    {% set profiler_markup_version = profiler_markup_version|default(1) %}
    {% if profiler_markup_version == 1 %}
        <span class="label">
            <span class="icon">
                <img alt="Soap"
                     src="data:image/png;base64,"/></span>
                <strong>Soap</strong>
            {% if collector.calls | length > 0 %}
                <span class="count">
                    <span>{{ collector.calls | length }}</span>
                    <span>{{ collector.totalTiming }} ms</span>
                </span>
            {% endif %}
        </span>
    {% else %}
        <span class="label {{ collector.calls | length == 0 ? 'disabled' }}">
            <span class="icon">{{ include('@PhproSoapClientBridge/Icon/soap.svg') }}</span>
            <strong>Soap</strong>
        </span>
    {% endif %}
{% endblock %}

{% block panel %}
    {% set profiler_markup_version = profiler_markup_version|default(1) %}
    <style>
        .soap-response-container {
            background: #F5F5F5;
            margin: .5em 0;
            padding: 1em;
        }

        {% if profiler_markup_version == 1 %}
        .hidden {
            display: none;
        }

        .soap-requests-table td, .queries-table th {
            vertical-align: top;
        }

        .soap-requests-table td > div {
            margin-bottom: 6px;
        }

        {% endif %}
    </style>

    <h2>Request Metrics</h2>

    {% if profiler_markup_version == 1 %}
        <table>
            <tbody>
            <tr>
                <th style="width: 20%;">Soap Requests</th>
                <td>{{ collector.calls | length }}</td>
            </tr>
            <tr>
                <th>Total Time</th>
                <td>{{ '%0.2f'|format(collector.totalTiming) }} ms</td>
            </tr>
            </tbody>
        </table>
    {% else %}
        <div class="metrics">
            <div class="metric">
                <span class="value">{{ collector.calls | length }}</span>
                <span class="label">Soap Requests</span>
            </div>

            <div class="metric">
                <span class="value">{{ '%0.2f'|format(collector.totalTiming) }} ms</span>
                <span class="label">Total Time</span>
            </div>
        </div>
    {% endif %}

    <h2>Requests</h2>

    {% if collector.calls is empty %}
        <div class="empty">
            <p>No Soap requests were performed.</p>
        </div>
    {% else %}
        <table class="alt soap-requests-table">
            <thead>
            <tr>
                <th class="nowrap">#</th>
                <th class="nowrap">Time</th>
                <th style="width: 100%;">Query</th>
            </tr>
            </thead>
            <tbody>
            {% for request in collector.calls %}
                <tr>
                    <td class="nowrap">{{ loop.index }}</td>
                    <td class="nowrap">{{ '%0.2f'|format(request.timing) }} ms </td>
                    <td>
                        <div>{{ request.clientName }}::{{ request.method }}</div>
                        <a href="#" class="btn btn-link text-small sf-toggle" data-toggle-selector="#soap-request-headers-{{ loop.index }}" data-toggle-alt-content="Hide headers"{{ profiler_markup_version == 1 ? 'onclick="return toggle(this);"' }}>
                            Show headers
                        </a>

                        <div id="soap-request-headers-{{ loop.index }}" class="hidden sf-toggle-content"><pre>{{ request.requestHeaders }}</pre></div>
                        
                        <a href="#" class="btn btn-link text-small sf-toggle" data-toggle-selector="#soap-request-body-{{ loop.index }}" data-toggle-alt-content="Hide body"{{ profiler_markup_version == 1 ? 'onclick="return toggle(this);"' }}>
                            Show body
                        </a>
                        
                        <div id="soap-request-body-{{ loop.index }}" class="hidden sf-toggle-content"><pre>{{ request.requestBody }}</pre></div>

                        <a href="#" class="btn btn-link text-small sf-toggle" data-toggle-initial="display" data-toggle-selector="#soap-response-{{ loop.index }}" data-toggle-alt-content="Show response"{{ profiler_markup_version == 1 ? 'onclick="return toggle(this);"' }}>
                            Hide response
                        </a>
                        
                        <div id="soap-response-{{ loop.index }}" class="sf-toggle-content soap-response-container">
                            <div style="margin-bottom: 15px;"><pre>{{ request.responseHeaders }}</pre></div>
                            <div><pre>{{ request.responseBody }}</pre></div>
                        </div>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>

        {% if profiler_markup_version == 1 %}
            <script type="text/javascript">//<![CDATA[
                function toggle(target) {
                    var targetSelector = target.getAttribute('data-toggle-selector');
                    var targetDataAltContent = target.getAttribute('data-toggle-alt-content');
                    var targetElement = document.querySelector(targetSelector);
                    target.setAttribute('data-toggle-alt-content', target.innerHTML);
                    if (targetElement.style.display != 'block') {
                        targetElement.style.display = 'block';
                        target.innerHTML = targetDataAltContent;
                    } else {
                        targetElement.style.display = 'none';
                        target.innerHTML = targetDataAltContent;
                    }
                    return false;
                }
                //]]></script>
        {% endif %}
    {% endif %}
{% endblock %}