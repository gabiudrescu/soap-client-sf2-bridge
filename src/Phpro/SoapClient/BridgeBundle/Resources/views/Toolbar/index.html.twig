{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <span class="icon">
            <img height="18" style="padding:5px 0 5px 0;"
                 src="data:image/png;base64,"/>
        </span>
        <span class="sf-toolbar-status">{{ collector.calls | length }} </span> {{ collector.totalTiming}} ms
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <ul>
            {% for call in collector.calls %}
                <li>{{ call.method }}: <strong>{{ call.timing }}</strong> ms</li>
            {% endfor %}
            </ul>
        </div>
    {% endset %}

    {% set link = true %}

    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig') }}
{% endblock %}

{#{% block head %}#}

{#{% endblock %}#}

{% block menu %}
    {# This left-hand menu appears when using the full-screen profiler. #}
    <span class="label">
        <span class="icon">
            <img src="data:image/png;base64,"/>
        </span>
        <strong>Soap</strong>
    </span>
{% endblock %}

{% block panel %}
    {# Optional, for showing the most details. #}
    <h2>Soap Calls</h2>
    {% for call in collector.calls %}
        <table>
            <tr>
                <th colspan="2">{{ call.clientName }}::{{ call.method }}<span style="float: right;">{{ call.timing }} ms</span></th>
            </tr>
            <tr>
                <th>Request Headers</th>
                <td><pre>{{ call.requestHeaders }}</pre></td>
            </tr>
            <tr>
                <th>Request Body</th>
                <td><pre>{{ call.requestBody }}</pre></td>
            </tr>
            <tr>
                <th>Request Object</th>
                <td>{{ dump(call.request) }}</td>
            </tr>
            <tr>
                <th>Response Headers</th>
                <td><pre>{{ call.responseHeaders }}</pre></td>
            </tr>
            <tr>
                <th>Response Body</th>
                <td><pre>{{ call.responseBody }}</pre></td>
            </tr>
            <tr>
                <th>Response Object</th>
                <td>{{ dump(call.response) }}</td>
            </tr>
        </table>
    {% endfor %}
{% endblock %}